	<title>Trivia Arena</title>
	
	<script src="jquery/dist/jquery.min.js"></script>
		
	<link rel="stylesheet" href="bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" href="bootstrap/dist/css/bootstrap-theme.min.css">
	<script src="bootstrap/dist/js/bootstrap.min.js"></script>
	
	<link rel="stylesheet" href="example/style.css">
	
</head>
<body>

<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">Trivia Arena</a>
    </div>
    <div id="navbar" class="navbar-collapse collapse">
      <ul class="nav navbar-nav">
        <li><a href="#home">Home</a></li>
        <li><a href="#choose">Choose</a></li>
        <li><a href="#play">Play</a></li>
        <li><a href="#scores">Scores</a></li>
        <li><a href="#about">About</a></li>
      </ul>
    </div>
  </div>
</nav>

<section id="home">
  <div class="container">
    <div class="header">
    	<div class="jumbotron">
    		<h1>Welcome to Trivia Arena!</h1>
    		<h3>Trivia Arena is an interactive quiz game where can have fun, battle your friends, and learn all at the same time!</h3>
    	</div>
    </div>
  </div>
</section>

<section id="choose">
  <div class="container">
    <h2>Choose your set of terms here</h2>
    <p>
      <button type="button" class="btn btn-lg btn-default">Set 1</button>
      <button type="button" class="btn btn-lg btn-default">Set 2</button>
      <button type="button" class="btn btn-lg btn-default">Set 3</button>
    </p>
  </div>
</section>

<section id="play">
  <div class="container">
    <h2>Game</h2>
    
    
    	<div id='container'>
			  <div id='title'>
				  <h1>Dynamic Quiz</h1>
			  </div>
   			  <br/>
  			  <div id='quiz'></div>
    		  <div class='button' id='next'><a href='#'>Next</a></div>
    		  <div class='button' id='prev'><a href='#'>Prev</a></div>
    		  <div class='button' id='start'> <a href='#'>Start Over</a></div>
    	</div>

		<script src="example/index.js"></script>
    
    
  </div>
</section>

<section id="scores">
  <div class="container">
    <h2>Scoreboard</h2>
      <div class="row">
        <div class="col-md-6">
          <table class="table table-bordered">
            <thead>
              <tr>
                <th>#</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Score</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>1</td>
                <td>John</td>
                <td>Smith</td>
                <td>4321</td>
              </tr>
              <tr>
                <td>2</td>
                <td>Jack</td>
                <td>Spencer</td>
                <td>1234</td>
              </tr>
              <tr>
                <td>3</td>
                <td>Mark</td>
                <td>Twain</td>
                <td>1111</td>
              </tr>
            </tbody>
          </table>
        </div>
  </div>
</section>

